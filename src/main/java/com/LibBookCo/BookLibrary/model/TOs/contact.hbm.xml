<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="Model.To">
    <class name="ContactTO" table="CONTACT_TBL">
        <id name="id" type="long" >
            <column name="ID" sql-type="NUMBER"/>
            <generator class="increment"/>
        </id>
        <property name="name" type="java.lang.String" >
            <column name="NAME" sql-type="NVARCHAR2(20)" />
        </property>
        <property name="family" type="java.lang.String">
            <column name="FAMILY" sql-type="NVARCHAR2(20)"/>
        </property>
        <property name="phoneNumber" type="java.lang.String">
            <column name="PHONENUMBER" sql-type="NVARCHAR2(20)"/>
        </property>
        <property name="address" type="java.lang.String">
            <column name="ADDRESS" sql-type="NVARCHAR2(20)"/>
        </property>
    </class>
</hibernate-mapping>
